@using Flashcards.Features.Flashcard.Model

<div class="plaintext-content">
    <FlashcardFrame TopLeftText="Q">@card.Question</FlashcardFrame>
    <FlashcardFrame TopLeftText="A" StartAsHidden="true">@card.Answer</FlashcardFrame>
</div>

@code {
    [Parameter]
    public FlashcardPlaintextV1Card card { get; set; }
}
